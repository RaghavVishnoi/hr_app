<%= form_for(project_team) do |f| %>
  <% if project_team.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(project_team.errors.count, "error") %> prohibited this exam from being saved:</h2>

      <ul>
      <% project_team.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div class="field form-group">
    <%= f.label :project_id, "Project" %>
    <%= f.select :project_id, Project.all.collect {|p| [ p.name, p.id ] }, {}, class: "form-control"  %>
  </div>

  <div class="actions">
    <%= f.submit "Save Project Team", class: "btn btn-primary" %>
  </div>
<% end %>
