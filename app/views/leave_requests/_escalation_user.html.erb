<div id="escalationUser" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Escalation Employee</h4>
      </div>
      <hr>
      <div class="modal-body">
        <p>Please select the employee you want to escalate to.</p>
        <p><%= f.select :escalation_user_id, [["Select Employee",0]]+Employee.all.where.not(id: current_employee.id)}, {}, class: "form-control" %></p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-success" id="submitEscalatedUser" style="margin-right: 11%;">Submit</button>
      </div>
    </div>

  </div>
</div>